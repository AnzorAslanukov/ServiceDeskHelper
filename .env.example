# Database Configuration
POSTGRES_DB=servicedeskhelper
POSTGRES_USER=your_username_here
POSTGRES_PASSWORD=your_secure_password_here
POSTGRES_PORT=5432
POSTGRES_HOST=localhost

# Databricks Configuration
DATABRICKS_EMBEDDING_URL=https://your-workspace.databricks.com/serving-endpoints/your-embedding-endpoint
DATABRICKS_TEXT_GENERATION_URL=https://your-workspace.databricks.com/serving-endpoints/your-text-generation-endpoint
DATABRICKS_API_KEY=your_databricks_api_key_here

# Database Connection String (for application use)
DATABASE_URL=postgresql://your_username_here:your_secure_password_here@localhost:5432/servicedeskhelper

# Vector Database Configuration
VECTOR_DIMENSION=1024

# Application Configuration
LOG_LEVEL=INFO
DEBUG=false

# File Processing Configuration
ONENOTE_DATA_DIR=./data/onenote/workbooks
DEBUG_FILE_PATH=./debug.txt

# Athena API Configuration
ATHENA_CLIENT_ID=api_client_id
ATHENA_USERNAME=uphs_username
ATHENA_PASSWORD=uphs_password
ATHENA_AUTH_URL=https://athena.api.auth.com/api/
ATHENA_BASE_URL=https://athena.api.endpoint.com/api/
ATHENA_JSON_TEMPLATE='[{"key":"value"}]'